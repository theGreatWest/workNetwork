// Pagination 
nav(aria-label='Page navigation example')
    ul.pagination.pagination-template.d-flex.justify-content-center
        li.page-item
            a.page-link(href='#') 
                i.fa.fa-angle-left
        li.page-item.active
            a.page-link(href='#') 1
        li.page-item
            a.page-link(href='#') 2
        li.page-item
            a.page-link(href='#') 3
        li.page-item
            a.page-link(href='#') 
                i.fa.fa-angle-right